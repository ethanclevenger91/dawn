{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "subtitle": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "a97d63b7-e61a-48b4-8b81-c98e3b27a37a": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "{{ product.metafields.my_fields.short_description }}"
          }
        },
        "336c02db-a2f3-4a1e-8d12-98a0e28077b1": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product__accordion accordion\" {{ block.shopify_attributes }}>\n              <details>\n                <summary>\n                  <div class=\"summary__title\">\n                    {% render 'icon-accordion', icon: 'pants' %}\n                    <h2 class=\"h4 accordion__title\">\n                      Description\n                    <\/h2>\n                  <\/div>\n                  {% render 'icon-caret' %}\n                <\/summary>\n                <div class=\"accordion__content rte\">\n  {{ product.description }}\n                <\/div>\n              <\/details>\n            <\/div>\n<div class=\"product__accordion accordion\" {{ block.shopify_attributes }}>\n              <details>\n                <summary>\n                  <div class=\"summary__title\">\n                    {% render 'icon-accordion', icon: 'ruler' %}\n                    <h2 class=\"h4 accordion__title\">\n                      Size Guide\n                    <\/h2>\n                  <\/div>\n                  {% render 'icon-caret' %}\n                <\/summary>\n                <div class=\"accordion__content rte\">\n  <div data-app=\"eastsideco_sizeGuides\"\n                data-shop=\"{{ shop.permanent_domain }}\"\n                data-tags=\"{{ product.tags | join:',' | escape }}\"\n                data-metafields=\"{{ product.metafields.esc-size-matters.tag }}\"\n                data-popup=\"button\">\n            <\/div>\n                <\/div>\n              <\/details>\n            <\/div>"
          }
        },
        "88abe9c6-eb6f-40d1-9d36-a5b8250f776a": {
          "type": "custom_liquid",
          "settings": {
            "custom_liquid": "<div class=\"product__accordion accordion\" {{ block.shopify_attributes }}>\n              <details>\n                <summary>\n                  <div class=\"summary__title\">\n                    {% render 'icon-accordion', icon: 'star' %}\n                    <h2 class=\"h4 accordion__title\">\n                      Product Reviews\n                    <\/h2>\n                  <\/div>\n                  {% render 'icon-caret' %}\n                <\/summary>\n                <div class=\"accordion__content rte\">\n  <div id=\"shopify-product-reviews\" data-id=\"{{product.id}}\">{{ product.metafields.spr.reviews }}<\/div>\n                <\/div>\n              <\/details>\n            <\/div>"
          }
        },
        "ae065407-6136-400b-a6f0-0d595d57dd32": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Shipping & Returns",
            "content": "<p>Items leave our shop Tuesday - Saturday. Shipping method can be selected at checkout. We offer USPS, UPS, and DHL for international shipments. Please double-check your size and review <a href=\"\/policies\/refund-policy\" title=\"Refund Policy\">our refund policy<\/a>. <\/p>",
            "page": "",
            "icon": "question_mark"
          }
        },
        "variant_picker": {
          "type": "variant_picker",
          "settings": {
            "picker_type": "button"
          }
        },
        "quantity_selector": {
          "type": "quantity_selector",
          "settings": {
          }
        },
        "buy_buttons": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true
          }
        },
        "share": {
          "type": "share",
          "settings": {
            "share_label": "Share"
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "subtitle",
        "price",
        "a97d63b7-e61a-48b4-8b81-c98e3b27a37a",
        "336c02db-a2f3-4a1e-8d12-98a0e28077b1",
        "88abe9c6-eb6f-40d1-9d36-a5b8250f776a",
        "ae065407-6136-400b-a6f0-0d595d57dd32",
        "variant_picker",
        "quantity_selector",
        "buy_buttons",
        "share"
      ],
      "settings": {
        "enable_sticky_info": true,
        "hide_variants": false,
        "enable_video_looping": false
      }
    },
    "product-recommendations": {
      "type": "product-recommendations",
      "settings": {
        "heading": "You may also like",
        "image_ratio": "adapt",
        "show_secondary_image": false,
        "add_image_padding": false,
        "show_image_outline": false,
        "show_vendor": true,
        "show_rating": false
      }
    }
  },
  "order": [
    "main",
    "product-recommendations"
  ]
}
