          "settings": {
            "custom_liquid": "<div class=\"product__accordion accordion\" {{ block.shopify_attributes }}>\n              <details>\n                <summary>\n                  <div class=\"summary__title\">\n                    {% render 'icon-accordion', icon: 'pants' %}\n                    <h2 class=\"h4 accordion__title\">\n                      Description\n                    <\/h2>\n                  <\/div>\n                  {% render 'icon-caret' %}\n                <\/summary>\n                <div class=\"accordion__content rte\">\n  {{ product.description }}\n                <\/div>\n              <\/details>\n            <\/div>"
          }
        },
        "ae065407-6136-400b-a6f0-0d595d57dd32": {
          "type": "collapsible_tab",
          "settings": {
            "heading": "Shipping & Returns",
            "icon": "question_mark",
            "content": "<p>Items leave our shop Monday-Friday. Shipping method can be selected at checkout.<\/p><p>Please double-check your size. <\/p><p>Most items may only be returned for store credit. Exceptions apply. Please review <a href=\"\/policies\/refund-policy\" title=\"Refund Policy\">our refund policy<\/a> for full details.<\/p>",
