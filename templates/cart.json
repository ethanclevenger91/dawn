    "fa27bad4-a073-48b4-9756-075dc8d94371": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% if cart.total_price > 230 and cart.total_price < 250 %}\n<div class=\"page-width\">\n<h2>You've Almost Unlocked Free Shipping!<\/h2>\n<p>Orders over $250 ship free. Here are some great add-ons if you want to qualify.<\/p>\n    <slider-component class=\"slider-mobile-gutter page-width-desktop\">\n      <ul\n        id=\"Slider-{{ section.id }}\"\n        class=\"grid product-grid contains-card contains-card--product grid--4-col-desktop grid--1-col-tablet-down slider slider--tablet grid--peek\"\n        role=\"list\"\n        aria-label=\"{{ 'general.slider.name' | t }}\"\n      >\n        {%- for product in section.settings.collection.products limit: 4 -%}\n          <li\n            id=\"Slide-{{ section.id }}-{{ forloop.index }}\"\n            class=\"grid__item slider__slide\">\n            {% render 'card-product',\n              card_product: product,\n              media_aspect_ratio: section.settings.image_ratio,\n              image_shape: section.settings.image_shape,\n              show_secondary_image: section.settings.show_secondary_image,\n              show_vendor: section.settings.show_vendor,\n              show_rating: true,\n              show_quick_add: true,\n              section_id: section.id\n            %}\n          <\/li>\n        {%- endfor -%}\n      <\/ul>\n      {%- if show_mobile_slider or show_desktop_slider -%}\n        <div class=\"slider-buttons no-js-hidden\">\n          <button\n            type=\"button\"\n            class=\"slider-button slider-button--prev\"\n            name=\"previous\"\n            aria-label=\"{{ 'general.slider.previous_slide' | t }}\"\n            aria-controls=\"Slider-{{ section.id }}\"\n          >\n            {% render 'icon-caret' %}\n          <\/button>\n          <div class=\"slider-counter caption\">\n            <span class=\"slider-counter--current\">1<\/span>\n            <span aria-hidden=\"true\"> \/ <\/span>\n            <span class=\"visually-hidden\">{{ 'general.slider.of' | t }}<\/span>\n            <span class=\"slider-counter--total\">{{ products_to_display }}<\/span>\n          <\/div>\n          <button\n            type=\"button\"\n            class=\"slider-button slider-button--next\"\n            name=\"next\"\n            aria-label=\"{{ 'general.slider.next_slide' | t }}\"\n            aria-controls=\"Slider-{{ section.id }}\"\n          >\n            {% render 'icon-caret' %}\n          <\/button>\n        <\/div>\n      {%- endif -%}\n    <\/slider-component>\n{% endif %}",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
    }
